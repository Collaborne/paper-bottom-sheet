{
	"compilerOptions": {
		"target": "es2017",
		"declaration": true,
		"outDir": "./dist",
		"module": "esnext",
		"moduleResolution": "node",
		"lib": ["es2017", "dom"],
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"experimentalDecorators": true
	},
	"include": [
		"src/**/*"
	],
	"exclude": [],
}
